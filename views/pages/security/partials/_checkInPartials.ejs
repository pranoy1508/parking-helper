<br>
<br>
<div class="row">
    <h5>
        Reservations Requests for the day :
    </h5>
</div><br />
<div class="row">
    <div class="input-group mb-4"><input type="search" placeholder="Search here with Guest Name ...."
            aria-describedby="button-addon5" class="form-control" id="txt_guestName" onkeyup="getReservationDetailsForGuest()">
        <div class="input-group-append">
            <button id="button-searchGuest" type="button" class="btn btn-primary"> <i
                    class="fa fa-search"> </i>
            </button>
        </div>
    </div>
</div><br>
<table id="tbl_check_in" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <th>Date</th>
        <th>Name</th>
        <th>Vehicle Type</th>
        <th>Office Location</th>
        <th>Parking Location</th>
        <th>Action</th>
    </thead>
    <% items.reservations.forEach(function (details) { %>
        <tr>
            <td>
                <%= details.reservationDate %>
            </td>
            <td>
                <%= details.guestName %>
            </td>
            <td>
                <%= details.vehicleType==0 ? "2Wheeler" : "4Wheeler" %>
            </td>
            <td>
                <%= details.officeLocation %>
            </td>
            <td>
                <%= details.parkingLocation %>
            </td>
            <td><button type="button" class="btn btn-success" onclick="checkInGuest('<%=details._id%>')">Check-In</button></td>
        </tr>
        <%})%>
</table>