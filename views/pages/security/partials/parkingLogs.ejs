<div class="d-flex justify-content-between">
    <h1 class="commanHeading">Enter Parking Details :<b>
        </b></h1>
    <button type="button" onclick="addUser()" class="btn btn-success fa fa-solid fa-plus" name="btnAddUser"> Refresh </button>
</div>
<br />
<div class="card">
    <div class="body table-responsive">
        <div class="container d-none" name="div_add_logs">
            <h5>Addition of Parking Logs : </h5>
            <br>
            <table id="user_add_tbl" class="table table-striped table-bordered" style="width:100%">
                <tbody>
                    <tr>
                        <td>
                            <input type="text" class="form-control" name="add_userName"
                                placeholder="Enter the user Name">
                        </td>
                        <td>
                            <input type="text" class="form-control" name="password"
                                placeholder="Password will be auto-generated" readonly>
                        </td>
                        <td>
                            <select class="form-select form-control" name="add_user_role"
                                aria-label="Default select example">
                                <option value="USER">User</option>
                                <option value="OWNER">Owner</option>
                                <option value="ADMIN">Admin</option>
                            </select>
                        </td>
                    </tr>
                </tbody>
            </table>
            <button type="button" onclick="saveUser()" class="btn btn-primary" name="btnSaveUser">Save</button>
            <button type="button" onclick="cancelUserAddition()" class="btn btn-danger" style="float: right;"
                name="btnCancelUserAddition">Cancel</button>
        </div>
    </div>
</div>