<br />
<br />
<div class="d-flex justify-content-between">
    <h1 class="commanHeading">Register your vehicle :<b>
        </b></h1>
</div>
<br />
<div class="card">
    <div class="body table-responsive">
        <div class="container">
            <table id="user_add_tbl" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <th>UserName</th>
                    <th>Employee Id</th>
                    <th>Vehicle Number</th>
                    <th>Vehicle Type</th>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <input type="email" class="form-control" name="reg_userName"
                                placeholder="Enter the user Name" readonly value="<%= items.vehicleInfo[0].ownerName %>">
                        </td>
                        <td>
                            <input type="email" class="form-control" name="reg_empId"
                                placeholder="Enter your Employee Id" value="<%= items.vehicleInfo[0].ownerEmployeeId %>">
                        </td>
                        <td>
                            <input type="text" class="form-control" name="reg_vehicleNumber"
                                placeholder="Enter your vehicle number" value="<%= items.vehicleInfo[0].vehicleNumber %>">
                        </td>
                        <td>
                            <select class="form-select form-control" name="add_user_role"
                                aria-label="Default select example">
                                <option value=0  <%= items.vehicleInfo[0].vehicleType ==0 ? 'selected' : '' %>>2Wheeler</option>
                                <option value=1 <%= items.vehicleInfo[0].vehicleType ==1 ? 'selected' : '' %>>4Wheeler</option>
                            </select>
                        </td>
                    </tr>
                </tbody>
            </table><br />
            <button type="button" onclick="saveUser()" class="btn btn-primary" style="float: right;" name="btnSaveUser">Save</button>
        </div>
    </div>
</div>