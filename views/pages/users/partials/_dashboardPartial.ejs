<br />
<br />
<div class="d-flex justify-content-between">
    <h1 class="commanHeading font-weight-bold">Parking Info :<b>
        </b></h1>
</div>
<div class="card">
    <div class="body table-responsive">
        <div class="container">
            <table id="parking_info_tbl" class="table table-striped table-bordered" style="width:100%">
                <tbody>
                    <tr>
                        <th style="font-size: 20px;">Select LTIMindtree Location : </th>
                        <th><select class="form-select form-control" id="location_ddl_sec"
                                aria-label="Default select example"
                                onchange="locationSelectionUpdated(this,'<%= JSON.stringify(items.officeDetails)  %>')">
                                <% items.officeDetails.forEach(function (location) { %>
                                    <option value='<%= location.OfficeLocation %>'>
                                        <%= location.OfficeLocation %>
                                    </option>
                                    <%})%>
                            </select></th>
                    </tr>
                    <tr>
                        <td style="font-size: 20px;font-weight: bold;">Select Parking Location :
                        </td>
                        <td><select class="form-select form-control" id="parking_location_ddl_usr"
                                aria-label="Default select example"
                                onchange="availableParkingLocationChanged('<%= JSON.stringify(items.officeDetails)  %>')">
                                <% items.officeDetails[0].ParkingLocations.forEach(function (parking) { %>
                                    <option value='<%= parking.LocationId %>'>
                                        <%= parking.LocationName %>
                                    </option>
                                    <%})%>
                            </select></td>
                    </tr>
                </tbody>
            </table>
            <br />
            <div id="parkingInfo_div">
                <table id="parking_avl_tbl" class="table table-striped table-bordered" style="width:100%">
                    <tr>
                        <th>
                            <span class="text-danger" style="font-size: larger;">Total 2 Wheeler Parking : <input
                                    type="number" class="text-danger font-weight-bold" disabled id="totTwoLbl"
                                    value="<%= items.officeDetails[0].ParkingLocations[0].TotalTwoWheelerCount %>">
                                </input>
                            </span>
                            <span class="text-success" style="font-size: larger;;float: right;">Available
                                2 Wheeler Parking: <input type="number" id="avlTwoLbl"
                                    class="text-success font-weight-bold" disabled value="<%=
                                    items.officeDetails[0].ParkingLocations[0].TotalTwoWheelerCount-items.officeDetails[0].ParkingLocations[0].BookedTwoWheelerCount
                                    %>"> </input>
                            </span>
                        </th>
                    </tr>
                    <tr>
                        <td style="text-align:center" id="dtl_two_wheeler">
                            <% let bookedValue=0;for (var i=0; i
                                <=items.officeDetails[0].ParkingLocations[0].TotalTwoWheelerCount ; i++ ) { %>
                                <% if (i%20==0) { %>
                                    </br>
                                    <%} else { if(bookedValue <
                                        items.officeDetails[0].ParkingLocations[0].BookedTwoWheelerCount) {
                                        bookedValue++;%>
                                        <i class='fas fa-parking'
                                            style='font-size:48px;color:green'></i>
                                        <%}else{ %>
                                            <i class='fas fa-parking'
                                                style='font-size:48px;color:#8080809c'></i>
                                            <% }} %>
                                                <% } %>
                        </td>
                    </tr>
                    <tr>
                        <th>
                            <span class="text-danger" style="font-size: larger;">Total 4 Wheeler Parking
                                : <input type="number" class="text-danger font-weight-bold" disabled id="totFourLbl"
                                    value="<%= items.officeDetails[0].ParkingLocations[0].TotalFourWheelerCount %>">
                                </input></span>
                            <span class="text-success" style="font-size: larger;float: right;">Available
                                4 Wheeler Parking: <input type="number" id="avlFourLbl"
                                    class="text-success font-weight-bold" disabled value="<%= items.officeDetails[0].ParkingLocations[0].TotalFourWheelerCount-items.officeDetails[0].ParkingLocations[0].BookedFourWheelerCount
                                                                    %>"> </input>
                            </span>
                        </th>
                    </tr>
                    <tr>
                        <td style="text-align:center" id="dtl_four_wheeler">
                            <% let bookedValueFour=0;for (var i=0; i <=items.officeDetails[0].ParkingLocations[0].TotalFourWheelerCount ; i++ ) { %>
                                <% if (i%20==0) { %>
                                    </br>
                                    <%} else { if(bookedValueFour < items.officeDetails[0].ParkingLocations[0].BookedFourWheelerCount) { bookedValueFour++;%>
                                        <i class='fas fa-parking' style='font-size:48px;color:green'></i>
                                        <%}else{ %>
                                            <i class='fas fa-parking' style='font-size:48px;color:#8080809c'></i>
                                            <% }} %>
                                                <% } %>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>